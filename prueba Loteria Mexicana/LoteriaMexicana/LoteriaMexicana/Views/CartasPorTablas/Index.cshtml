@model IEnumerable<LoteriaMexicana.Models.Entities.CartasPorTabla>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PosicionF)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PosicionC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCartaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTablaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PosicionF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PosicionC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCartaNavigation.IdCarta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTablaNavigation.IdTabla)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCartasPorTabla">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCartasPorTabla">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCartasPorTabla">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
